{
  "extends": "@featul/tsconfig/react-library.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "paths": {
      "@featul/ui/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}